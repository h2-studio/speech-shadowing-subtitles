[{"start":4.17,"end":5.82,"text":"Welcome to English in a Minute!","phonetics":[[0,7,"'wɛlkəm"],[11,7,"'ɪŋglɪʃ"],[24,6,"'mɪnɪt"]]},{"start":6.39,"end":7.83,"text":"Tape is helpful.","phonetics":[[0,4,"teɪp"],[8,7,"'hɛlpfəl"]]},{"start":8.28,"end":10.212,"text":"It keeps things stuck together","phonetics":[[3,5,"kip"],[9,6,"θɪŋz"],[16,5,"stək"],[22,8,"tʊ'ɡɛðər"]]},{"start":10.7793333333333,"end":13.7803333333333,"text":"But red tape can cause problems.","phonetics":[[0,3,"bət"],[4,3,"rɛd"],[8,4,"teɪp"],[13,3,"kæn"],[17,5,"kɔz"],[23,8,"'prɑbləm"]]},{"start":14.4096666666667,"end":16.7896666666667,"text":"So, are you getting your new driver's license today?","phonetics":[[4,3,"ɑr"],[8,3,"ju"],[12,7,"'ɡɛtɪŋ"],[20,4,"jɔr"],[25,3,"nu"],[38,7,"'laɪsəns"],[46,5,"tə'deɪ"]]},{"start":17.3325,"end":17.9025,"text":"Yes!","phonetics":[[0,3,"jɛs"]]},{"start":18.2925,"end":20.9445,"text":"And I remembered my birth certificate.","phonetics":[[0,3,"ænd"],[20,5,"bɜrθ"],[26,11,"sər'tɪfɪkət"]]},{"start":21.6225,"end":22.8225,"text":"But you have your eye test, right?","phonetics":[[0,3,"bət"],[4,3,"ju"],[8,4,"hæv"],[13,4,"jɔr"],[18,3,"aɪ"],[22,4,"tɛst"],[28,5,"raɪt"]]},{"start":23.1825,"end":24.5425,"text":"And proof of address?","phonetics":[[0,3,"ænd"],[4,5,"pruf"],[13,7,"ə'drɛs"]]},{"start":25.1025,"end":25.9125,"text":"Eye test?","phonetics":[[0,3,"aɪ"],[4,4,"tɛst"]]},{"start":26.3625,"end":27.7025,"text":"Proof of address?","phonetics":[[0,5,"pruf"],[9,7,"ə'drɛs"]]},{"start":28.6125,"end":30.5125,"text":"So much red tape!","phonetics":[[3,4,"məʧ"],[8,3,"rɛd"],[12,4,"teɪp"]]},{"start":31.0425,"end":31.7625,"text":"Red tape?","phonetics":[[0,3,"rɛd"],[4,4,"teɪp"]]},{"start":32.2923333333333,"end":35.5123333333333,"text":"Or important department of motor vehicles requirements?","phonetics":[[3,9,"ɪm'pɔrtənt"],[13,10,"dɪ'pɑrtmənt"],[27,5,"'moʊtər"],[33,8,"'viɪkəl"],[42,12,"rɪ'kwaɪrmənt"]]},{"start":36.355,"end":37.468,"text":"It's red tape, Dan.","phonetics":[[0,4,"ɪts"],[5,3,"rɛd"],[9,4,"teɪp"],[15,3,"dæn"]]},{"start":38.4825,"end":42.6571666666667,"text":"In large organizations like governments or corporations,","phonetics":[[3,5,"lɑrdʒ"],[9,13,",ɔrgənɪ'æeɪʃən"],[23,4,"laɪk"],[28,11,"'ɡəvərnmənt"],[43,12,",kɔrpə'reɪʃən"]]},{"start":42.6811666666667,"end":46.6525,"text":"there are lots of rules that may feel unnecessary.","phonetics":[[0,5,"ðɜr"],[6,3,"ɑr"],[10,4,"lɑts"],[18,5,"rulz"],[24,4,"ðæt"],[29,3,"meɪ"],[33,4,"fil"],[38,11,"ən'nɛsɪ,sɛri"]]},{"start":47.3325,"end":52.4925,"text":"Those rules can even prevent things from being done on time or at all.","phonetics":[[0,5,"ðoʊz"],[6,5,"rulz"],[12,3,"kæn"],[16,4,"'ivən"],[21,7,"prɪ'vɛnt"],[29,6,"θɪŋz"],[36,4,"frʌm"],[41,5,"'biɪŋ"],[47,4,"dən"],[55,4,"taɪm"],[66,3,"ɔl"]]},{"start":53.0325,"end":55.4145,"text":"We call these rules red tape.","phonetics":[[3,4,"kɔl"],[8,5,"ðiz"],[14,5,"rulz"],[20,3,"rɛd"],[24,4,"teɪp"]]},{"start":56.2651666666667,"end":57.7261666666667,"text":"And that's English in a Minute!","phonetics":[[0,3,"ænd"],[4,6,"ðæts"],[11,7,"'ɪŋglɪʃ"],[24,6,"'mɪnɪt"]]}]